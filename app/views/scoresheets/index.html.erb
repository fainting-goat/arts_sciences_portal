<p id="notice"><%= notice %></p>

<h1>Scoresheets</h1>

<table>
  <thead>
    <tr>
      <th>Score</th>
      <th>User</th>
      <th>Entry</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scoresheets.each do |scoresheet| %>
      <tr>
        <td><%= scoresheet.score_id %></td>
        <td><%= scoresheet.user_id %></td>
        <td><%= scoresheet.entry_id %></td>
        <td><%= link_to 'Show', scoresheet %></td>
        <td><%= link_to 'Edit', edit_scoresheet_path(scoresheet) %></td>
        <td><%= link_to 'Destroy', scoresheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scoresheet', new_scoresheet_path %>
